<div class="container mt-5">
  <h1> Clients </h1>
  <div class="row">
    <% @clients.each do |client| %>
      <div class="col-4">
        <div class="food-card w-100">
          <%= link_to client.clientname.capitalize, client_path(client)%>
        </div>
      </div>
    <% end %>
  </div>
</div>
